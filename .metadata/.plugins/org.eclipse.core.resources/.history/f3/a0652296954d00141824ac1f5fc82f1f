package raphaelpantaleao.katabanckocr.builders;

import javax.swing.JTextArea;

public class ScrollableJTextAreaBuilder {
	private JTextArea textArea;

	private ScrollableJTextAreaBuilder() {
		textArea = new JTextArea();
	}

	public ScrollableJTextAreaBuilder newScrollableJTextAreaBuilder() {
		ScrollableJTextAreaBuilder builder = new ScrollableJTextAreaBuilder();
		return this;
	}
}
